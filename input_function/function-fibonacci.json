{
  "function": "fibonacci",
  "return-type": "int",
  "parameters": [
    {
      "parameter": "n",
      "type": "int"
    }
  ],
  "conditions": [
    {
      "condition": "n == 0",
      "action": "0"
    },
    {
      "condition": "n == 1",
      "action": "1"
    },
